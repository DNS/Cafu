    CONTROL "Status -- OK",                                                                      DLG1_STATUSBAR,            "STATIC",   SS_CENTER | SS_SUNKEN | WS_CHILD | WS_VISIBLE, 5, 288, 276, 10

    CONTROL "OK",                                                                                IDOK,                      "BUTTON",   BS_DEFPUSHBUTTON | WS_CHILD | WS_VISIBLE | WS_TABSTOP, 75, 307, 40, 14
    CONTROL "Cancel",                                                                            IDCANCEL,                  "BUTTON",   BS_PUSHBUTTON | WS_CHILD | WS_VISIBLE | WS_TABSTOP, 172, 307, 40, 14
