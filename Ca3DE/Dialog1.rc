    CONTROL "Ping Server",                                                                       DLG1_CL_PINGSV,            "BUTTON",   BS_PUSHBUTTON | WS_CHILD | WS_VISIBLE | WS_TABSTOP, 190, 156, 40, 12
    CONTROL "Send NOP",                                                                          DLG1_CL_SENDNOP,           "BUTTON",   BS_PUSHBUTTON | WS_CHILD | WS_VISIBLE | WS_TABSTOP, 234, 156, 40, 12
