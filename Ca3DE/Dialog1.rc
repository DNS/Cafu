#include <windows.h>
#include "Dialog1.h"



MainDialog DIALOG DISCARDABLE 207, 46, 287, 329
STYLE DS_MODALFRAME | DS_CENTER | WS_CAPTION | WS_VISIBLE | WS_POPUPWINDOW
