    lightmap    $lightmap

    shaderParamExpr 0.0         // Fog color (RGB).
    shaderParamExpr 0.4
    shaderParamExpr 1.0

    shaderParamExpr 0.000066    // Fog density.

 // shaderParamExpr 0.0         // Fog start (optional).
 // shaderParamExpr 1.0         // Fog end   (optional).
}
