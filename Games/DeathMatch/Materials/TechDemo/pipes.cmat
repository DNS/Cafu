TechDemo/pipes/a_pipecap2a_d
{
    diffusemap  Textures/TechDemo/pipes/a_pipecap2a_d_diff.png
    normalmap   Textures/TechDemo/pipes/a_pipecap2a_d_norm.png
    specularmap Textures/TechDemo/pipes/a_pipecap2a_d_spec.png
    lightmap    $lightmap

    alphaTest 0.9
    noShadows

    clip nothing    // Normally this is an opaque decal (clip radiance, sight), but this one has alphaTest enabled!
}

