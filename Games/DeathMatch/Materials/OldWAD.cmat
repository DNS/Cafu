}


Textures/OldWAD/po_glassdrkdrt
{
    diffusemap  Textures/OldWAD/po_glassdrkdrt_diff.png
    normalmap   hm2nm(Textures/generic/_white_diff.png, 1.0)
    specularmap Textures/generic/_white_diff.png
    lightmap    $lightmap

    blendFunc src_alpha one_minus_src_alpha
    alpha     0.4
    noShadows       // Cast no shadows, so that when this material is used e.g. in doorframes, light shines through.
    clip players, monsters, moveables, ik, projectiles   // Don't clip bspPortals, radiance or sight.
}


